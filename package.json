{
  "name": "ds-venturists-typescript",
  "version": "1.0.0",
  "description": "DS Venturists - Dubai Scholars' flagship entrepreneurship competition website built with TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "start": "npx webpack serve --mode development",
    "build": "npx webpack --mode production",
    "dev": "npx webpack serve --mode development",
    "serve": "npx webpack-dev-server --mode development",
    "watch": "npx webpack --mode development --watch",
    "type-check": "npx tsc --noEmit",
    "lint": "npx eslint src --ext .ts,.tsx",
    "format": "npx prettier --write src/**/*.{ts,tsx,css,html}",
    "clean": "npx rimraf dist"
  },
  "keywords": [
    "entrepreneurship",
    "competition",
    "typescript",
    "dubai-scholars",
    "education"
  ],
  "author": "Mahin Kashyap",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "chart.js": "^4.3.3",
    "gsap": "^3.12.2"
  }
}
