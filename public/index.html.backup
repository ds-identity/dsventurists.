    <script>
        // Clean navigation system
        (function() {
            'use strict';
            
            function showPage(pageId) {
                console.log('Showing page:', pageId);
                
                try {
                    // Hide all sections
                    const allSections = document.querySelectorAll('.page-section');
                    allSections.forEach(section => {
                        section.classList.remove('active', 'force-active');
                    });
                    
                    // Show target section
                    const target = document.getElementById(pageId);
                    if (target) {
                        target.classList.add('force-active');
                        console.log('Successfully showing:', pageId);
                    } else {
                        console.error('Page not found:', pageId);
                        // Fallback to home
                        const home = document.getElementById('home');
                        if (home) {
                            home.classList.add('force-active');
                        }
                    }
                    
                    // Update navigation states  
                    document.querySelectorAll('.dock-item, .mobile-nav-item').forEach(item => {
                        item.classList.remove('active');
                    });
                    document.querySelectorAll(`[data-page="${pageId}"]`).forEach(item => {
                        item.classList.add('active');
                    });
                    
                    // Close mobile menu
                    const menu = document.getElementById('dockMenu');
                    if (menu && menu.classList.contains('active')) {
                        menu.classList.remove('active');
                        const icon = document.getElementById('dockToggleIcon');
                        if (icon) icon.className = 'fas fa-bars';
                    }
                    
                } catch (error) {
                    console.error('Navigation error:', error);
                }
                
                return false;
            }
            
            // Expose globally
            window.showPage = showPage;
            
            // Initialize navigation
            function initializeNavigation() {
                console.log('Initializing navigation...');
                showPage('home');
            }
            
            if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', initializeNavigation);
            } else {
                initializeNavigation();
            }
            
            window.addEventListener('load', () => {
                setTimeout(initializeNavigation, 100);
            });
            
        })();
        
        // EMERGENCY FIXES FOR MEDIA PAGE AND LOGO
        (function() {
            'use strict';
            
            // Force the media page to display with emergency fixes
            function emergencyMediaFix() {
                const mediaPage = document.getElementById('media');
                if (mediaPage) {
                    mediaPage.classList.add('force-active');
                    mediaPage.style.display = 'block';
                    mediaPage.style.opacity = '1';
                    mediaPage.style.visibility = 'visible';
                    
                    // Force all children to be visible
                    const allElements = mediaPage.querySelectorAll('*');
                    allElements.forEach(el => {
                        if (getComputedStyle(el).display === 'none') {
                            el.style.display = 'block';
                        }
                        el.style.opacity = '1';
                        el.style.visibility = 'visible';
                    });
                    
                    console.log('Emergency media fix applied!');
                }
            }
            
            // PRECISE LOGO QUALITY FIX - ONLY ACTUAL LOGO IMAGES
            function emergencyLogoFix() {
                // More precise selectors to avoid affecting text
                const logoSelectors = [
                    'img.hero-logo',
                    'img.dock-logo', 
                    '.sponsor-logo img',
                    'img[src*="logo.webp"]',
                    'img[src*="logo.jpg"]',
                    'img[src*="logo.jpg"]',
                    'img[src*="logo.jpeg"]',
                    'img[alt*="Logo"][src]',
                    'img[alt*="logo"][src]',
                    '.hero-logo-container img[src]',
                    '.logo-backdrop + img[src]'
                ];
                
                let totalLogos = 0;
                
                logoSelectors.forEach(selector => {
                    const logos = document.querySelectorAll(selector);
                    logos.forEach(logo => {
                        // Only apply to actual IMG elements
                        if (logo.tagName === 'IMG') {
                            // Apply ALL the fixes
                            logo.style.imageRendering = 'crisp-edges';
                            logo.style.msInterpolationMode = 'nearest-neighbor';
                            logo.style.filter = 'contrast(1.4) brightness(1.2) saturate(1.3) drop-shadow(0 0 1px rgba(255,255,255,0.5))';
                            logo.style.webkitFilter = 'contrast(1.4) brightness(1.2) saturate(1.3)';
                            logo.style.transform = 'translateZ(0) scale(1.001)';
                            logo.style.webkitTransform = 'translateZ(0) scale(1.001)';
                            logo.style.backfaceVisibility = 'hidden';
                            logo.style.webkitBackfaceVisibility = 'hidden';
                            logo.style.willChange = 'transform';
                            logo.style.objectFit = 'contain';
                            logo.style.objectPosition = 'center';
                            logo.style.imageOrientation = 'from-image';
                            logo.style.webkitUserSelect = 'none';
                            logo.style.mozUserSelect = 'none';
                            logo.style.userSelect = 'none';
                            logo.style.pointerEvents = 'none';
                            logo.style.webkitTouchCallout = 'none';
                            
                            // Force reload the image to apply fixes
                            const src = logo.src;
                            logo.src = '';
                            logo.src = src;
                            
                            totalLogos++;
                        }
                    });
                });
                
                console.log(`üöÄ PRECISE LOGO FIX applied to ${totalLogos} logo images!`);
                return totalLogos;
            }
            
            // Apply fixes immediately and on navigation
            document.addEventListener('DOMContentLoaded', () => {
                emergencyLogoFix();
                if (window.location.hash === '#media') {
                    emergencyMediaFix();
                }
            });
            
            window.addEventListener('hashchange', () => {
                if (window.location.hash === '#media') {
                    setTimeout(emergencyMediaFix, 100);
                }
                emergencyLogoFix();
            });
            
            // Override the showPage function to include emergency fixes
            const originalShowPage = window.showPage;
            window.showPage = function(pageId) {
                const result = originalShowPage ? originalShowPage(pageId) : false;
                
                if (pageId === 'media') {
                    setTimeout(emergencyMediaFix, 50);
                }
                emergencyLogoFix();
                
                return result;
            };
            
            // Force fixes every 2 seconds for critical issues
            setInterval(() => {
                emergencyLogoFix();
                if (window.location.hash === '#media' || document.querySelector('#media.force-active')) {
                    emergencyMediaFix();
                }
            }, 2000);
            
            // IMMEDIATE LOGO FIX ON LOAD
            if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', () => {
                    console.log('üî• IMMEDIATE LOGO FIX ON DOM READY');
                    emergencyLogoFix();
                });
            } else {
                console.log('üî• IMMEDIATE LOGO FIX NOW');
                emergencyLogoFix();
            }
            
            // Additional immediate fix after a brief delay
            setTimeout(() => {
                console.log('üî• DELAYED IMMEDIATE LOGO FIX');
                emergencyLogoFix();
            }, 100);
            
            setTimeout(() => {
                console.log('üî• SECOND DELAYED LOGO FIX');
                emergencyLogoFix();
            }, 500);
            
            setTimeout(() => {
                console.log('üî• THIRD DELAYED LOGO FIX');
                emergencyLogoFix();
            }, 1000);
            
        })();
    </script>
    
